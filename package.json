{
  "name": "@lenne.tech/nest-server",
  "version": "0.0.6",
  "description": "Modern, fast, powerful Node.js web framework in TypeScript based on Nest with a GraphQL API and a connection to MongoDB (or other databases).",
  "keywords": [
    "node",
    "nest",
    "server",
    "mongodb",
    "graphql",
    "typescript"
  ],
  "author": "Kai Haase",
  "homepage": "http://lenne.tech",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prepublish": "npm run prestart:prod",
    "prestart:prod": "rimraf dist && npm run build",
    "reinit": "rm -f package-lock.json && rm -rf node_modules && npm cache clean --force && npm i",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:dev": "nodemon",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lenneTech/nest-server"
  },
  "bugs": {
    "url": "https://github.com/lenneTech/nest-server/issues"
  },
  "engines": {
    "node": ">= 12.4.0"
  },
  "dependencies": {
    "@nestjs/common": "6.3.1",
    "@nestjs/core": "6.3.1",
    "@nestjs/graphql": "6.2.4",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "6.3.1",
    "@nestjs/testing": "6.3.1",
    "@nestjs/typeorm": "6.1.2",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.134",
    "@types/multer": "1.3.7",
    "@types/node": "12.0.8",
    "@types/passport": "1.0.0",
    "@types/supertest": "2.0.7",
    "apollo-server-express": "2.6.4",
    "bcrypt": "3.0.6",
    "class-transformer": "0.2.3",
    "class-validator": "0.9.1",
    "fastify": "2.5.0",
    "graphql": "14.3.1",
    "graphql-subscriptions": "1.1.0",
    "jest": "24.8.0",
    "json-to-graphql-query": "1.9.0",
    "lodash": "4.17.11",
    "mongodb": "3.2.7",
    "nodemon": "1.19.1",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "prettier": "1.18.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "rxjs": "6.5.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.18.0",
    "type-graphql": "0.17.4",
    "typeorm": "0.2.16",
    "typescript": "3.5.2"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    }
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ]
}
